                                    <span id="btnEliminarDomingo" onclick="eliminarDomingo(1);">
                                        <i class="far fa-times-circle fa-2x"></i>
                                    </span>
                                </div>
                            <% } %>
                            <div id="divHorariosDomingo"></div>
                        </div>
                        <div class="text-center">
                            <span onclick="agregarDomingo();" id="addDomingo" class="btn btn-link ">Agregar otro horario</span>
