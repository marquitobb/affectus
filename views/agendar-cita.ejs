                                <label class="ml-auto mr-5">Hora de termino</label>
                            </div>
                            <div class="form-group mb-0 text-center ">
                                <% if(horario.InicioLunes.length > 0) { %>
                                    <% for (let i = 0; i < horario.InicioLunes.length; i++) { %>
                                        <% const element = horario.InicioLunes[i] %>
                                        <% const element2 = horario.FinLunes[i] %>
